<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.leo.dao.TagMapper">

    <select id="findById" resultType="TagDO">
        SELECT *
        FROM tag
        WHERE oId = #{id}
    </select>

</mapper>